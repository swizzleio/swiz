{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Print Version",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd",
            "cwd": "${workspaceFolder}",
            "args": [
                "--appconfig",
                "file://test/data/cloudformation/app-config.yaml",
                "version"
            ]
        },
        {
            "name": "Create awesomeEnv",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd",
            "cwd": "${workspaceFolder}",
            "args": [
                "--appconfig",
                "file://test/data/cloudformation/app-config.yaml",
                "env",
                "deploy",
                "--name",
                "awesomeEnv"
            ]
        }
    ]
}